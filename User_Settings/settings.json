{
  "material-icon-theme.folders.color": "#ff3838",
  "editor.wordWrap": "on",
  "editor.mouseWheelZoom": true,
  "material-icon-theme.folders.associations": {
    "C": "Cobol",
    "C++": "Base",
    "Data_Structure_&_Algorithms": "Database",
    "Web_Development": "Global",
    "App_Development": "Mobile",
    "React": "React-components",
    "React-components": "React-components",
    "NextJS": "React-components",
    "NodeJS": "Node",
    "Git_&_Github": "Github",
    "Github": "Github",
    "Practice": "Custom",
    "Pictures": "images",
    "Picture": "images",
    "Tutorial": "Class",
    "Java": "Less",
    "Kotlin": "Mobile",
    "Machine_Learning": "Core",
    "Roadmap": "Routes",
    "Wordpress": "Wordpress",
    "ExpressJS": "Node",
    "MongoDB": "Database",
    "MERN": "Stack",
    "Exercise": "Tasks",
    "Statistics": "Storybook",
    "Firebase": "Firebase",
    "Redux": "Redux-actions",
    "Redux-actions": "Redux-actions",
    "Redux-reducer": "Redux-reducer",
    "Redux-store": "Redux-store",
    "Aritficial_Intelligence": "Core",
    "AI": "Core",
    "React-Native": "React-components",
    "Dart": "Less",
    "Visual_Studio_Code": "vscode",
    "Django": "resolver",
    "Blockchain_Development": "Container",
    "Nginx": "ngrx-store",
    "UX_&_UI": "vm",
    "bloc": "container",
    "cubit": "circleci",
    "Notes": "docs",
    "presentations": "animation"
  },
  "editor.formatOnSave": true,
  "screencastMode.verticalOffset": 0,
  "screencastMode.fontSize": 35,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "editor.fontLigatures": true,
  "dart.previewFlutterUiGuides": true,
  "editor.inlineSuggest.enabled": true,
  "workbench.colorTheme": "Dracula",
  "html.validate.styles": false,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "remote.SSH.remotePlatform": {
    "digitaloceantesting": "linux"
  }
}
